<div class="contents row">
  <% @tweets.each do |tweet| %>
    <div class="row" >
      <div class="col s12 m6 offset-m3">
        <div class="card">
          <div class="card-content deep-orange-text">
            <p><%= tweet.contents %></p>
          </div>
          <div class="card-action">
          </div>
        </div>
      </div>
    </div>
  <% end %>
  <%= paginate(@tweets) %>  
</div>